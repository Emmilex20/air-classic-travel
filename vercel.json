{
  "framework": "react-router", 
  "buildCommand": "cd frontend && pnpm install --no-frozen-lockfile && pnpm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && pnpm install --store=node_modules/.pnpm-store --no-frozen-lockfile",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}